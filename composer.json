{
    "name": "mindy/pagination",
    "type": "library",
    "description": "Mindy Pagination Component",
    "license": "MIT",
    "homepage": "https://mindy-cms.com",
    "autoload": {
        "psr-4": {
            "Mindy\\Pagination\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Pagination\\Tests\\": ""
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "symfony/http-foundation": "~3.0|~4.0",
        "symfony/routing": "~3.0|~4.0",
        "phpunit/phpunit": "~4.0"
    }
}
